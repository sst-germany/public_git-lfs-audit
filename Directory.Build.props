<Project>
  <!-- DO MODIFY: -->
  <PropertyGroup>
	  <SstVersion_3dig>1.3.1</SstVersion_3dig>
	  <SstVersion_4dig>$(SstVersion_3dig).0</SstVersion_4dig>
	  <SstProduct>git-lfs-audit</SstProduct>
    <!-- Bsp. <DefineConstants>MY_CONSTANT</DefineConstants> -->
  </PropertyGroup>

  <!-- Automatic build of assembly information: DO NOT MODIFY! -->
  <PropertyGroup>
    <!--to identify the used Directory.Build.props-->
    <SstDirectoryBuildPropsName>Product: $(SstProduct)</SstDirectoryBuildPropsName>
    <SstBuildNumberExtension>build($([System.DateTime]::Now.ToString("yy"))$([System.DateTime]::Now.DayOfYear.ToString("0"))-$([System.DateTime]::Now.TimeOfDay.TotalSeconds.ToString("0")))</SstBuildNumberExtension>
    <AssemblyVersion>$(SstVersionSuffix)</AssemblyVersion>
    <Version>$(SstVersionSuffix)$(SstVersionExtension)</Version>
    <SourceRevisionId>$(SstBuildNumberExtension)</SourceRevisionId>
    <Company>SST Scheubeck GmbH</Company>
    <Authors>SST Scheubeck GmbH</Authors>
    <Copyright>Copyright Â© SST Scheubeck GmbH $([System.DateTime]::Now.ToString("yyyy"))</Copyright>
    <Product Condition="'$(Configuration)'=='Debug'">$(SstProduct) (DEBUG)"</Product>
    <Product Condition="'$(Configuration)'=='Release'">$(SstProduct)"</Product>
  </PropertyGroup>

</Project>
